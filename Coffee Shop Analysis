        "import pandas as pd\n",
        "import matplotlib.pyplot as plt"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "#creatin a simple dataset\n",
        "data = {\n",
        "    'day': ['mon', 'tue', 'wed', 'thu', 'fri', 'sat', 'sun'],\n",
        "    'sales_USD': [150, 200, 230, 239, 300, 260, 400],\n",
        "    'customers': [15, 20, 24, 15, 23, 35, 30,]\n",
        "}\n",
        "df=pd.DataFrame(data)\n",
        "#analysis\n",
        "df['spends_per_customers'] = df['sales_USD']/df['customers']"
      ],
      "metadata": {

        "# to find the percentage difference between spending per customers on different dazs\n",
        "mean_value = df['spends_per_customers'].mean()\n",
        "max_value = df['spends_per_customers'].max()\n",
        "percentage_difference = (max_value - mean_value)/mean_value* 100\n",
        "print(percentage_difference)"
